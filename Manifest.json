{
    "name" : "Shiven's Ad Blocker",
    "version" : "1.0",
    "description" : "Blocks ads on Youtube",
  

    "content scripts" : [
        {
            "matches" : ["<all_urls>"],
            "run_at" : ["document_idle"],
            "js" : ["content.js"]
        }
    ],

    "permissions" : [
            "tabs",
            "activeTab"
        ],

    
    "icons": {
        "16": "Icons/16x16.png",
        "48": "Icons/48x48.png",
        "128": "Icons/128x128.png"
        },
        
    "manifest_version" : 2
}